{
  "name": "common-forms-toolkit-frontend",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "node --max_old_space_size=6144 node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "clean": "rm -rf coverage dist",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install",
    "serve": "vue-cli-service serve",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit --verbose --forceExit --detectOpenHandles --maxWorkers=10",
    "test:unit:short": "vue-cli-service test:unit --forceExit --detectOpenHandles --maxWorkers=10",
    "pretest": "npm run lint",
    "purge": "rm -rf node_modules"
  },
  "dependencies": {
    "@bcgov/bc-sans": "1.0.1",
    "axios": "0.25.0",
    "core-js": "3.20.3",
    "keycloak-js": "12.0.4",
    "moment": "2.29.1",
    "nprogress": "0.2.0",
    "validator": "13.7.0",
    "vue": "2.6.14",
    "vue-router": "3.5.3",
    "vuetify": "2.6.3",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-eslint": "4.5.15",
    "@vue/cli-plugin-router": "4.5.15",
    "@vue/cli-plugin-unit-jest": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.3.0",
    "axios-mock-adapter": "1.20.0",
    "babel-eslint": "10.1.0",
    "eslint": ">= 6.8.0 < 7",
    "eslint-plugin-prettier": ">= 3.4.0 < 4",
    "eslint-plugin-vue": "6.2.2",
    "eslint-plugin-vuetify": "1.0.0-beta.6",
    "prettier": "2.5.1",
    "sass": ">= 1.32.13 < 1.33.0",
    "sass-loader": ">= 10.2.0 < 11.0.0",
    "vue-cli-plugin-vuetify": "2.4.5",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3"
  }
}
