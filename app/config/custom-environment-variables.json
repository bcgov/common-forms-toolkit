{
  "db": {
    "database": "DB_DATABASE",
    "host": "DB_HOST",
    "username": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "port": "DB_PORT"
  },
  "frontend": {
    "apiPath": "FRONTEND_APIPATH",
    "basePath": "FRONTEND_BASEPATH",
    "env": "FRONTEND_ENV",
    "geocoder": {
      "endpoint": "FRONTEND_GEOCODER_ENDPOINT"
    },
    "keycloak": {
      "clientId": "FRONTEND_KC_CLIENTID",
      "realm": "FRONTEND_KC_REALM",
      "serverUrl": "FRONTEND_KC_SERVERURL"
    },
    "orgbook": {
      "endpoint": "FRONTEND_ORGBOOK_ENDPOINT"
    }
  },
  "objectStorage": {
    "accessKeyId": "OBJECTSTORAGE_ACCESSKEYID",
    "bucket": "OBJECTSTORAGE_BUCKET",
    "endpoint": "OBJECTSTORAGE_ENDPOINT",
    "key": "OBJECTSTORAGE_KEY",
    "secretAccessKey": "OBJECTSTORAGE_SECRETACCESSKEY"
  },
  "server": {
    "apiPath": "SERVER_APIPATH",
    "basePath" : "SERVER_BASEPATH",
    "bodyLimit": "SERVER_BODYLIMIT",
    "keycloak": {
      "adminClientId": "SERVER_KC_ADMINCLIENTID",
      "adminClientSecret": "SERVER_KC_ADMINCLIENTSECRET",
      "clientId": "SERVER_KC_CLIENTID",
      "clientSecret": "SERVER_KC_CLIENTSECRET",
      "realm": "SERVER_KC_REALM",
      "serverUrl": "SERVER_KC_SERVERURL"
    },
    "logLevel": "SERVER_LOGLEVEL",
    "morganFormat": "SERVER_MORGANFORMAT",
    "port": "SERVER_PORT",
    "uploads": {
      "dir": "SERVER_UPLOADS_DIR",
      "fieldName": "SERVER_UPLOADS_FIELD_NAME",
      "maxFileCount": "SERVER_UPLOADS_MAX_FILE_COUNT",
      "maxFileSize": "SERVER_UPLOADS_MAX_FILE_SIZE"
    }
  },
  "serviceClient": {
    "commonServices": {
      "ches": {
        "endpoint": "SC_CS_CHES_ENDPOINT"
      },
      "cdogs": {
        "endpoint": "SC_CS_CDOGS_ENDPOINT"
      },
      "tokenEndpoint": "SC_CS_TOKEN_ENDPOINT",
      "username": "SC_CS_USERNAME",
      "password": "SC_CS_PASSWORD"
    }
  }
}
